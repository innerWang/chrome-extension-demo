{
  "name":"plugin-example",
  "description": "",
  "version":"1.0",
  "background": {
    "scripts": ["extension/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "css": ["content_script.css"],
      "js": ["extension/content_script.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon/16.png",
      "32": "icon/32.png",
      "48": "icon/48.png",
      "128": "icon/128.png"
    }
  },
  "permissions": ["activeTab","storage"],
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "options_page": "options.html",
  "manifest_version":2
}